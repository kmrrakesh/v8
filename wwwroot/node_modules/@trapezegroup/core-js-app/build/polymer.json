{
  "entrypoint": "index.html",
  "shell": "index.js",
  "fragments": [],
  "sources": [
    "index.html",
    "index.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": false,
        "minify": true,
        "transformModulesToAmd": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": false,
      "basePath": true
    }
  ],
  "autoBasePath": true,
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}